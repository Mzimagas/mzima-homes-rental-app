{
  "name": "kodirent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev:staging": "cp .env.staging .env.local && npm run dev",
    "build:staging": "cp .env.staging .env.local && npm run build",
    "test:staging": "cp .env.staging .env.local && npm run test",
    "analyze:staging": "cp .env.staging .env.local && node scripts/analyze-all-discovered-tables.js",
    "archive:staging": "cp .env.staging .env.local && node scripts/table-archive-operations.js",
    "test": "vitest run",
    "test:ui": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:staged": "vitest related --run",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "test-db": "node test-connection.js",
    "test-functions": "node test-functions.js",
    "migrate": "node scripts/run-migrations.js",
    "migrate:show": "node scripts/show-migrations.js",
    "migrate:verify": "node scripts/run-migrations.js --verify-only",
    "db:reset": "echo 'WARNING: This will reset your database. Use with caution.'",
    "db:seed": "node scripts/seed-database.js",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "validate": "npm run typecheck && npm run lint:strict && npm run test",
    "quality-gate": "node scripts/quality-gate.js",
    "quality-gate:ci": "npm run quality-gate -- --ci",
    "quality-dashboard": "node scripts/quality-dashboard.js",
    "security-audit": "npm audit --audit-level=moderate",
    "security-fix": "npm audit fix",
    "clean": "rm -rf .next out dist coverage",
    "clean:all": "npm run clean && rm -rf node_modules",
    "reinstall": "npm run clean:all && npm install",
    "check-updates": "npx npm-check-updates",
    "update-deps": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@jest/globals": "^30.0.5",
    "@marsidev/react-turnstile": "^1.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dotenv": "^17.2.1",
    "file-saver": "^2.0.5",
    "immer": "^10.1.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "next": "15.4.2",
    "posthog-js": "^1.259.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.4.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-coverage-report": "^1.1.1",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4",
    "webpack-bundle-analyzer": "^4.10.2",
    "zod": "^4.0.17"
  }
}
